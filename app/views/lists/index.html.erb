<div class="m-4 d-flex align-items-center justify-content-between">
  <h1>Mooooooovies! list</h1>
  <%= link_to "Add a list", new_list_path, class: "btn btn-dark"%>
</div>
<ul>
  <% @lists.each do |list| %>
    <li class="list-inline">
      <%= list.name %>
      <%=link_to list.name, list_path(list) %>
    <% end %>
  </li>
</ul>
